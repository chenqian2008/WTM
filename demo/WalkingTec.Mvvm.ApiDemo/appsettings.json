{
  "ConnectionStrings": [
    {
      "Key": "default",
      "Value": "Server=(localdb)\\mssqllocaldb;Database=ApiDemo_db;Trusted_Connection=True;MultipleActiveResultSets=true"
    }
  ],
  "DBType": "SqlServer", //使用的数据库，可选mysql,sqlserver
  "CookiePre": "WTM", //cookie前缀
  "EnableLog": true, //是否启用日志
  "LogExceptionOnly": false, //当启用日志的时候，是否只记录错误信息
  "IsQuickDebug": true, //是否启用debug模式
  "RPP": 20, //默认列表每页行数
  "SyncDb": true, //是否自动同步数据库
  "FileUploadOptions": {
    "UploadLimit": 20971520,
    "SaveFileMode": "Database", //上传文件的保存方式，可选Database,local,dfs
    "UploadDir": "D:\\" //当上传文件选择Local时，指定硬盘目录
  },
  "EncryptKey": "", //加密密钥
  "AppSettings": [
  ]
}

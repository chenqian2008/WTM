@model WalkingTec.Mvvm.Demo.ViewModels.ShipperInfoVMs.ShipperInfoVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.shipper_code" />
<wt:textbox field="Entity.shipper_name" />
<wt:textbox field="Entity.shipper_company" />
<wt:textbox field="Entity.shipper_countrycode" />
<wt:textbox field="Entity.shipper_province" />
<wt:textbox field="Entity.shipper_city" />
<wt:textbox field="Entity.shipper_street" />
<wt:textbox field="Entity.shipper_postcode" />
<wt:textbox field="Entity.shipper_areacode" />
<wt:textbox field="Entity.shipper_telephone" />
<wt:textbox field="Entity.shipper_mobile" />
<wt:textbox field="Entity.shipper_email" />
<wt:textbox field="Entity.shipper_fax" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
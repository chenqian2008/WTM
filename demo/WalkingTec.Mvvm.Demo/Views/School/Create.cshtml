@model WalkingTec.Mvvm.Demo.ViewModels.SchoolVMs.SchoolVM

<wt:form vm="@Model">
  <wt:row>
    <wt:textbox field="Entity.SchoolCode" />
    <wt:textbox field="Entity.SchoolName" />
    <wt:combobox field="Entity.SchoolType" />
    <wt:multiupload field="Entity.Photos" />
    <wt:ueditor field="Entity.Remark" height="300" />
  </wt:row>
  <wt:row align="AlignEnum.Right">
    <wt:submitbutton click="test()" text="运行js并提交" />
    <wt:submitbutton click="return test()" text="运行js不提交" />
    <wt:submitbutton />
    <wt:closebutton />
  </wt:row>
</wt:form>

<script>
  function test() {
    alert('test');
    return false;
  }
</script>

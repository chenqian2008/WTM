@model WalkingTec.Mvvm.Mvc.CodeGenVM


<wt:form vm="@Model" url="/_CodeGen/SetField">
    <wt:hidden field="EntryDir" />
    <wt:combobox field="SelectedModel" items="AllModels" />
    <wt:textbox field="Area" />
    <wt:textbox field="ModuleName" placeholder="请输入模块名称，比如xx管理"/>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="生成代码" />
        <wt:closebutton />
    </wt:row>

</wt:form>